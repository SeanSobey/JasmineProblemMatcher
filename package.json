{
  "name": "jasmine-problem-matcher",
  "displayName": "Jasmine Problem Matcher",
  "description": "A problem matcher to show Jasmine spec errors when running tests in the terminal.",
  "icon": "images/icon.png",
  "version": "1.0.2",
  "publisher": "seansobey",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeanSobey/JasmineProblemMatcher.git"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha test/**/*.spec.js",
    "debug-test": "node --inspect-brk=33295 --nolazy node_modules/mocha/bin/_mocha test/**/*.spec.js",
    "watch-build": "tsc --watch",
    "watch-test": "mocha --colors --watch test/**/*.spec.js",
    "vscode:prepublish": "",
    "compile": "",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "mocha": "^6.1.1",
    "source-map-support": "^0.5.11",
    "tslint": "^5.15.0",
    "tslint-divid": "^1.3.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^5.5.2",
    "typescript": "^3.4.2",
    "vsce": "^1.59.0",
    "vscode": "^1.15.0"
  },
  "author": "Sean Sobey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeanSobey/MochaProblemMatcher/issues"
  },
  "homepage": "https://github.com/SeanSobey/MochaProblemMatcher#readme",
  "categories": [
    "Languages"
  ],
  "contributes": {
    "problemMatchers": [
      {
        "name": "jasmine",
        "owner": "jasmine",
        "severity": "error",
        "fileLocation": [
          "absolute"
        ],
        "pattern": [
          {
            "regexp": "\\d+\\)\\s.+"
          },
          {
            "regexp": "\\s+Message:"
          },
          {
            "regexp": "\\s+(.*)",
            "message": 1
          },
          {
            "regexp": "\\s+Stack:"
          },
          {
            "regexp": "[^\\(]*\\((.*):(\\d+):(\\d+)\\)",
            "file": 1,
            "line": 2,
            "column": 3
          }
        ]
      }
    ]
  }
}
